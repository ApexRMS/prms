<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <module name="stsim-prms" displayName="ST-Sim PRMS" version="3.1.22" configVersion="1.0000" contentType="any">
    <transformers>
      <transformer name="stsim-prms" displayName="ST-Sim PRMS" isModel="True" isPrimary="True">
        <runtime>
          <transformer name="stsim-prms:runtime"/>
        </runtime>
      </transformer>
      <transformer name="stsim-prms:runtime" displayName="ST-Sim PRMS" className="SyncroSim.StochasticTime.StochasticTimeTransformer" classAssembly="SyncroSim.StochasticTime" externalProgramName="RScript" externalScriptName="transformer.R" environmentSheet="PRMS_R_Environment" configurationSheet="PRMS_RunControl">
        <datafeeds>
          <datafeed name="stsim-prms:env-datafeed" displayName="R" dataScope="Library">
            <datasheets>
              <datasheet name="PRMS_R_Environment" displayName="R">
                <columns>
                  <column name="EnvironmentID" dataType="Integer" isPrimary="True"/>
                  <column name="Name" dataType="String" allowDbNull="False"/>
                  <column name="Value" dataType="String" allowDbNull="False"/>
                </columns>
              </datasheet>
            </datasheets>
          </datafeed>
          <datafeed name="stsim-prms:basin-datafeed" displayName="Basin" dataScope="Project">
            <datasheets>
              <datasheet name="PRMS_Basin" displayName="Basin" valueMember="BasinID" displayMember="Name">
                <columns>
                  <column name="BasinID" dataType="Integer" isPrimary="True"/>
                  <column name="ProjectID" dataType="Integer"/>
                  <column name="Name" dataType="String"/>
                  <column name="Description" dataType="String" isOptional="True"/>
                </columns>
              </datasheet>
            </datasheets>
          </datafeed>
          <datafeed name="stsim-prms:run-control-datafeed" displayName="Run Control" dataScope="Scenario">
            <datasheets>
              <datasheet name="PRMS_RunControl" displayName="Run Control" isSingleRow="True">
                <columns>
                  <column name="RunControlID" dataType="Integer" isPrimary="True"/>
                  <column name="ScenarioID" dataType="Integer"/>
                  <column name="MinimumIteration" displayName="Minimum Iteration" dataType="Integer" defaultValue="1" validationType="WholeNumber" validationCondition="GreaterEqual" formula1="1"/>
                  <column name="MaximumIteration" displayName="Maximum Iteration" dataType="Integer" defaultValue="1" validationType="WholeNumber" validationCondition="GreaterEqual" formula1="1" format="d"/>
                  <column name="MinimumTimestep" displayName="Minimum Timestep" dataType="Integer" defaultValue="0" validationType="WholeNumber" validationCondition="GreaterEqual" formula1="0" format="d"/>
                  <column name="MaximumTimestep" displayName="Maximum Timestep" dataType="Integer" defaultValue="10" validationType="WholeNumber" validationCondition="GreaterEqual" formula1="0" format="d"/>
                </columns>
                <validations>
                  <validation validationType="LessEqual" columns="MinimumIteration|MaximumIteration"/>
                  <validation validationType="LessEqual" columns="MinimumTimestep|MaximumTimestep"/>
                </validations>
              </datasheet>
            </datasheets>
          </datafeed>
          <datafeed name="stsim-prms:input-stsim-scenario-datafeed" displayName="ST-Sim Scenario" dataScope="Scenario">
            <datasheets>
              <datasheet name="PRMS_InputSTSimScenario" displayName="ST-Sim Scenario" isSingleRow="True">
                <columns>
                  <column name="InputSTSimScenarioID" dataType="Integer" isPrimary="True"/>
                  <column name="ScenarioID" dataType="Integer"/>
                  <column name="STSimLibraryFile" displayName="ST-Sim Library File" dataType="String" isExternalFile="True" allowDbNull="False" externalFileFilter="ST-Sim Libraries|*.ssim"/>
                  <column name="ResultScenarioID" dataType="Integer"/>
                </columns>
              </datasheet>
            </datasheets>
          </datafeed>
          <datafeed name="stsim-prms:prms-input-datafeed" displayName="PRMS Inputs" dataScope="Scenario">
            <datasheets>
              <datasheet name="PRMS_PRMSInput" displayName="PRMS Inputs" isSingleRow="True">
                <columns>
                  <column name="PRMS_InputID" dataType="Integer" isPrimary="True"/>
                  <column name="ScenarioID" dataType="Integer"/>
                  <column name="BasinID" displayName="Basin" dataType="Integer" validationType="Datasheet" formula1="PRMS_Basin"/>
                  <column name="BoundaryShapeFile" displayName="Boundary Shape File" dataType="String" isExternalFile="True" allowDbNull="False" externalFileFilter="Shape Files|*.shp"/>
                  <column name="TemplateControlFile" displayName="Template Control File" dataType="String" isExternalFile="True" allowDbNull="False" externalFileFilter="Control Files|*.control"/>
                  <column name="TemplateParameterFile" displayName="Template Parameter File" dataType="String" isExternalFile="True" allowDbNull="False" externalFileFilter="Parameter Files|*.param"/>
                  <column name="InitialHRUValueFile" displayName="Initial HRU Value File" dataType="String" isExternalFile="True" allowDbNull="False" externalFileFilter="CSV Files|*.csv"/>
                  <column name="HRUCentroidFile" displayName="HRU Centroid File" dataType="String" isExternalFile="True" allowDbNull="False" externalFileFilter="TXT Files|*.txt"/>
                  <column name="AttributePRMSLookupFile" displayName="Attribute PRMS Lookup File" dataType="String" isExternalFile="True" allowDbNull="False" externalFileFilter="CSV Files|*.csv"/>
                </columns>
              </datasheet>
            </datasheets>
          </datafeed>
          <datafeed name="stsim-prms:climate-input-datafeed" displayName="Climate Inputs" dataScope="Scenario">
            <datasheets>
              <datasheet name="PRMS_ClimateInput" displayName="Climate Inputs" isSingleRow="True">
                <columns>
                  <column name="PRMS_ClimateInputID" dataType="Integer" isPrimary="True"/>
                  <column name="ScenarioID" dataType="Integer"/>
                  <column name="BasinID" displayName="Basin" dataType="Integer" validationType="Datasheet" formula1="PRMS_Basin"/>
                  <column name="Iteration" dataType="Integer"/>                  
                  <column name="InputClimateFile" displayName="Climate File" dataType="String" isExternalFile="True" allowDbNull="False" externalFileFilter="Climate Files|*.*"/>
                </columns>
              </datasheet>
            </datasheets>
          </datafeed>
        </datafeeds>
      </transformer>
    </transformers>
    <layouts>
      <layout name="system-forms:library-datafeeds" model="stsim-prms">
        <item name="system:files-datafeed"/>
        <item name="system:backup-datafeed"/>
        <item name="system:model-run-datafeed"/>
        <item name="stsim-prms:env-datafeed"/>
      </layout>
      <layout name="system-forms:project-datafeeds" model="stsim-prms">
        <item name="stsim-prms:basin-datafeed"/>
      </layout>
      <layout name="system-forms:scenario-datafeeds" model="stsim-prms">
        <item name="stsim-prms:run-control-datafeed"/>
        <item name="stsim-prms:input-stsim-scenario-datafeed"/>
        <item name="stsim-prms:prms-input-datafeed"/>        
        <item name="stsim-prms:climate-input-datafeed"/>         
      </layout>
    </layouts>
    <visualizers>
      <visualizer name="stsim-prms:env-datafeed-visualizer" target="stsim-prms:env-datafeed" className="SyncroSim.Core.Forms.RConfigurationDataFeedView" classAssembly="SyncroSim.Core.Forms"/>
    </visualizers>
  </module>
</configuration>